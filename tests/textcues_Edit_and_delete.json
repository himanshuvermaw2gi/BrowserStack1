{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://www.schoolcues.com/Default.aspx"
    },
    {
      "type": "pause",
      "waitTime": "9000"
    },

       {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='LoginView2_LoginUser_UserName']"
      },
      "text": "manas@panalinks.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='LoginView2_LoginUser_Password']"
      },
      "text": "manas123"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='LoginView2_LoginUser_LoginButton']"
      }
    },
{
      "type": "pause",
      "waitTime": "10000"
    },

    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "ContentPlaceHolder1_dlModuleList_lnkModuleName_11"
      }
    },
{
      "type": "pause",
      "waitTime": "8000"
    },

    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "ContentPlaceHolder1_GVCurrentTextCues_lnkTextCueEdit_0"
      }
    },
{
      "type": "pause",
      "waitTime": "8000"
    },

    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "ContentPlaceHolder1_btnSave"
      }
    },
{
      "type": "pause",
      "waitTime": "9000"
    },

    {
      "type": "dismissAlert"
    },
{
      "type": "pause",
      "waitTime": "9000"
    },

    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "ContentPlaceHolder1_GVCurrentTextCues_lnkDelete_0"
      }
    },
    {
      "type": "pause",
      "waitTime": "9000"
    },
    {
      "type": "dismissAlert"
    },
     {
      "type": "pause",
      "waitTime": "3000"
    },

    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "ContentPlaceHolder1_GVCurrentTextCues_lnkbtnActivate_0"
      }
    },
{
      "type": "pause",
      "waitTime": "9000"
    },

    {
      "type": "dismissAlert"
    },
{
      "type": "pause",
      "waitTime": "9000"
    }

  ]
}
