{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://www.schoolcues.com/"
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='top-nav']/div/div/div/div/ul/li[1]/a"
      }
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='top-nav']/div/div/div/div/ul/li[2]/a"
      }
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='video']/div/div/div[2]/div[2]/p/a"
      }
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='top-nav']/div/div/div/div/ul/li[3]/a"
      }
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='tour-3']/div[1]/div/a[2]"
      }
    },
    {
      "type": "pause",
      "waitTime": "7000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "html body.modal-open div#home.main-container section#features.section div.container div.row div.span12 div.row div.span6 div.extra-features div#myModal12.modal div.modal-footer a.btn"
      }
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
   {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='features']/div/div/div/div[3]/div[1]/div/a[2]"
      }
    }

]
}
