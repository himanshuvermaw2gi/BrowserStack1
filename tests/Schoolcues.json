{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://www.schoolcues.com/"
    },
     {
      "type": "pause",
      "waitTime": "4000"
    },

     {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='LoginView2_LoginUser_UserName']"
      },
      "text": "manas@panalinks.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='LoginView2_LoginUser_Password']"
      },
      "text": "manas123"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='LoginView2_LoginUser_LoginButton']"
      }
    },
 {
      "type": "pause",
      "waitTime": "4000"
    },


    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "ContentPlaceHolder1_dlModuleList_lnkModuleName_0"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "a[title=\"test01\"] > div.CalDayContent"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "ContentPlaceHolder1_txtDate"
      },
      "text": "01/23/2014"
    },

    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='ContentPlaceHolder1_pnldefault']/table/tbody/tr[1]/td[3]/table/tbody/tr[1]/td/select//option[3]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "ContentPlaceHolder1_btnUpdateMaster"
      }
    }
    
  ]
}
