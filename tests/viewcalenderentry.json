{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://www.schoolcues.com/"
    },
{
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "ContentPlaceHolder1_LoginView2_LoginUser_UserName"
      },
      "text": "manas@panalinks.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
