    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='more1/23/2014']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='ContentPlaceHolder1_ImgbtnBusinessHome']"
      }
    },
{
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='HeaderUC1_LoginView2_LoginStatus1']"
      }
    },
{
      "type": "pause",
      "waitTime": "3000"
    }
    
    
  ]
