{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://wafflehouse.com/"
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='art-main']/div[1]/div/div[6]/div/div[1]/div[1]/p/a/img"
      }
    },
{
      "type": "pause",
      "waitTime": "3000"
    },
   {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "addressline"
      },
      "text": "35004"
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#search_button"
      }
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Waffle House #825"
      }
    },
   {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "div.tab_label"
      }
    },
   {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "addressline"
      },
      "text": "AL"
    },
   {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//table/tbody/tr/td/div[1]/div[2]/div[1]/div[1]/div[6]/div/div[3]/div[2]/div/form/input[2]"
      }
    }
  ],
  "data": {
    "configs": {},
