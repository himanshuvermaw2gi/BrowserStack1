{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://www.schoolcues.com/Default.aspx"
    },
   {
      "type": "pause",
      "waitTime": "3000"
    },

    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='LoginView2_LoginUser_UserName']"
      },
      "text": "manas@panalinks.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='LoginView2_LoginUser_Password']"
      },
      "text": "manas123"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='LoginView2_LoginUser_LoginButton']"
      }
    },

{
      "type": "pause",
      "waitTime": "3000"
    },

    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "ParentViewText"
      }
    },
{
      "type": "pause",
      "waitTime": "3000"
    },

    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//table[@class='contentWidth']/tbody/tr[1]/td[1]/table/tbody/tr/td[6]/select//option[4]"
      }
    },
{
      "type": "pause",
      "waitTime": "8000"
    },

    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='more1/23/2014']"
      }
    },
{
      "type": "pause",
      "waitTime": "3000"
    }

  ]
}
