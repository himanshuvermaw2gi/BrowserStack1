{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://ignite.where2stageit.com/login.html"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "loginid"
      },
      "text": "riyer"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "ram@W2gi"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "submit"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "div > b"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"text\"]"
      },
      "text": "dennys"
    },
     {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//ul[@class='chzn-results']//li[.='dennys']"
      }
    },

    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "change_account"
      }
    },
                {
      "type": "pause",
      "waitTime": "4000"
    },

    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='Launchpad']/a"
      }
    },
{
      "type": "pause",
      "waitTime": "3000"
    },


    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "img[alt=\"Reputation\"]"
      }
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Configuration"
      }
    },
    {
      "type": "pause",
      "waitTime": "4000"
    },
    {
      "type": "verifyTextPresent",
      "text": "Negative Review Threshold"
    },
    {
      "type": "verifyTextPresent",
      "text": "View Subscribers"
    },
    {
      "type": "verifyTextPresent",
      "text": "Email Alert"
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='ReviewTable']/div[3]/table/tbody/tr[2]/td[2]/select//option[3]"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='ReviewTable']/div[3]/table/tbody/tr[3]/td[2]/select//option[5]"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='ReviewTable']/div[3]/table/tbody/tr[4]/td[2]/select//option[5]"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='ReviewTable']/div[3]/table/tbody/tr[6]/td[2]/select//option[5]"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Google+ Local"
    },
    {
      "type": "verifyTextPresent",
      "text": "Yelp"
    },
    {
      "type": "verifyTextPresent",
      "text": "Yahoo!"
    },
    
    {
      "type": "verifyTextPresent",
      "text": "Facebook"
    },
    {
      "type": "verifyElementValue",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='ReviewTable']/div[3]/table/tfoot/tr/td/input"
      },
      "value": "Save Changes"
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    
   {
      "type": "clickElement",
      "locator": {
        "type": "link text",
