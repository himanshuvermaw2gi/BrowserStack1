{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://www.schoolcues.com/"
    },
{
      "type": "pause",
      "waitTime": "3000"
    },
   {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='LoginView2_LoginUser_UserName']"
      },
      "text": "manas@panalinks.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='LoginView2_LoginUser_Password']"
      },
      "text": "manas123"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='LoginView2_LoginUser_LoginButton']"
      }
    },

{
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "ContentPlaceHolder1_dlModuleList_lnkModuleName_2"
      }
    },
{
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "ContentPlaceHolder1_btnAddNew"
      }
    },
{
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "ContentPlaceHolder1_txtNewsLetterSubject"
      },
      "text": "testA120"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "ContentPlaceHolder1_txtScheduleStartDate"
      },
      "text": "01/23/2014"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "id",
        "value": "ContentPlaceHolder1_cbCategory_0"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "id",
        "value": "ContentPlaceHolder1_clUserControl_RepeaterConList_rptCB_6"
      }
    },
    {
      "type": "sendKeysToElement",
      "locator": {
        "type": "xpath",
        "value": "//html/body"
      },
      "text": "this is a testing mail"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "ContentPlaceHolder1_btnSave"
      }
    }
  ]
}
